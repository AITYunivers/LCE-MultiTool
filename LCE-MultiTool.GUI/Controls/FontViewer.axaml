<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
			 d:DesignWidth="914"
			 d:DesignHeight="684"
             x:Class="LCE_MultiTool.FontViewer">
	<Border BorderBrush="{DynamicResource SemiColorBackground1}" BorderThickness="1" Margin="8">
		<Canvas>
			<Image x:Name="ViewerImage" Stretch="None" Canvas.Top="0" Canvas.Left="0" RenderOptions.BitmapInterpolationMode="None"/>
		</Canvas>
	</Border>
</UserControl>
